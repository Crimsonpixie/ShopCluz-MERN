{
  "name": "shopcluz",
  "version": "1.0.0",
  "description": "MERN shopping cart",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crimsonpixie/ShopCluz-Mern.git"
  },
  "author": "Shivkant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Crimsonpixie/ShopCluz-Mern/issues"
  },
  "homepage": "https://github.com/Crimsonpixie/ShopCluz-Mern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.10"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  }
}
